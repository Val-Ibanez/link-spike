{
  "name": "MyReactNativeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "android:banco-entre-rios": "react-native run-android --mode=bancoEntreRiosDebug",
    "android:banco-santa-cruz": "react-native run-android --mode=bancoSantaCruzDebug",
    "android:banco-santa-fe": "react-native run-android --mode=bancoSantaFeDebug",
    "android:link": "react-native run-android --mode=linkDebug",
    "ios:banco-entre-rios": "node scripts/prepare-env.js bancoEntreRios && react-native run-ios --scheme=BancoEntreRios",
    "ios:banco-santa-cruz": "node scripts/prepare-env.js bancoSantaCruz && react-native run-ios --scheme=BancoSantaCruz",
    "ios:banco-santa-fe": "node scripts/prepare-env.js bancoSantaFe && react-native run-ios --scheme=BancoSantaFe",
    "ios:link": "node scripts/prepare-env.js link && react-native run-ios --scheme=Link",
    "ios:config:banco-entre-rios": "node scripts/ios-config-setup.js bancoEntreRios",
    "ios:config:banco-santa-cruz": "node scripts/ios-config-setup.js bancoSantaCruz",
    "ios:config:banco-santa-fe": "node scripts/ios-config-setup.js bancoSantaFe",
    "ios:config:link": "node scripts/ios-config-setup.js link",
    "ios:clean-pods": "node scripts/ios-clean-pods.js",
    "ios:verify-config": "node scripts/verify-react-native-config.js",
    "clean": "npx react-native start --reset-cache",
    "clean:deep": "npm run clean && cd android && ./gradlew clean && cd ../ios && xcodebuild clean",
    "status": "./scripts/project-status.sh",
    "config:list": "node scripts/config-manager.js list",
    "config:validate": "node scripts/config-manager.js validate",


    "version:status": "node scripts/version-manager.js status",
    "build:release": "./scripts/build-release.sh",
    "keystores:generate": "./scripts/generate-keystores.sh",
    "keystores:info": "./scripts/keystore-info.sh",
    "dashboard:show": "node scripts/toggle-dashboard-features.js show",
    "dashboard:toggle": "node scripts/toggle-dashboard-features.js toggle",
    "dashboard:variant": "node scripts/toggle-dashboard-features.js variant",
    "dashboard:swap": "node scripts/toggle-dashboard-features.js swap",

    "setup:client": "./scripts/client-setup.sh"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/bottom-tabs": "^7.4.4",
    "@react-navigation/drawer": "^7.5.5",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/stack": "^7.4.4",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-config": "^1.5.5",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-svg": "^15.12.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@testing-library/react-native": "^13.3.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.0.0",
    "jest": "^29.6.3",
    "prettier": "^3.0.0",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
