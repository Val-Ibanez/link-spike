// Configuración para Banco Santa Fe
PRODUCT_BUNDLE_IDENTIFIER = com.myreactnativeapp.bancosantafe
PRODUCT_NAME = Banco Santa Fe
INFOPLIST_FILE = MyReactNativeApp/Info.plist
ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon.bancoSantaFe 

// Configuración para react-native-config
ENVFILE = $(SRCROOT)/../.env.bancoSantaFe

// Configuraciones de build específicas
SWIFT_OPTIMIZATION_LEVEL = -Onone
ENABLE_BITCODE = NO
CLANG_ENABLE_MODULES = YES
CLANG_ENABLE_OBJC_ARC = YES 