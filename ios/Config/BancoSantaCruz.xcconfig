// Configuración para Banco Santa Cruz
PRODUCT_BUNDLE_IDENTIFIER = com.myreactnativeapp.bancosantacruz
PRODUCT_NAME = Banco Santa Cruz
INFOPLIST_FILE = MyReactNativeApp/Info.plist
ASSETCATALOG_COMPILER_APPICON_NAME = AppIcon.bancoSantaCruz 

// Configuración para react-native-config
ENVFILE = $(SRCROOT)/../.env.bancoSantaCruz

// Configuraciones de build específicas
SWIFT_OPTIMIZATION_LEVEL = -Onone
ENABLE_BITCODE = NO
CLANG_ENABLE_MODULES = YES
CLANG_ENABLE_OBJC_ARC = YES 